{
  "name": "slmf-http-connector",
  "version": "0.0.0",
  "description": "Simple location message format HTTP Connector based on standard ISO/IEC 24730-1:2014 ",
  "main": "./index.js",
  "scripts": {
    "test": "jest --config jest.config.json",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel index.ts --out-dir . --extensions \".ts\" && babel src --out-dir lib --extensions \".ts\"",
    "build": "npm run build:types && npm run build:js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F-Army/SLMF-HTTP-connector.git"
  },
  "keywords": [
    "slmf",
    "http-connector",
    "rtls",
    "24730-1",
    "uwb"
  ],
  "author": {
    "name": "Michele Biondi",
    "email": "michelebiondi01@gmail.com",
    "url": "https://github.com/an4cr0n"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/F-Army/SLMF-HTTP-connector/issues"
  },
  "homepage": "https://github.com/F-Army/SLMF-HTTP-connector#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.9",
    "@types/joi": "^14.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "joi": "^14.0.4"
  }
}
